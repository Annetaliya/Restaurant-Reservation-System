{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client-side/my-app/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "DELETE", "PATCH", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://restaurant-reservation-system-annettes-projects-70970dfb.vercel.app",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Methods": "GET, POST, DELETE, PATCH, PUT, OPTIONS",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    
   {
      "src": "/static/(.*)",
      "dest": "client-side/my-app/build/static/$1"
    },
     {
      "src": "/favicon.ico",
      "dest": "client-side/my-app/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "client-side/my-app/build/manifest.json"
    },
    {
      "src": "/logo192.png", 
      "dest": "client-side/my-app/build/logo192.png"
    },
    {
      "src": "/robots.txt", 
      "dest": "client-side/my-app/build/robots.txt"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "client-side/my-app/build/index.html"
    }
  ]
}